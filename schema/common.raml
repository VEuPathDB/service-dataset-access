#%RAML 1.0 Library

types:
  DatastoreId:
    type: integer
    minimum: 1
  UserDetails:
    type: object
    properties:
      userId:
        type: integer
        format: int64
        minimum: 1
      firstName: string
      lastName: string
      organization: string
    additionalProperties: false
  RestrictionLevel:
    type: string
    enum:
      - public
      - limited
      - protected
      - controlled
      - admin
  ApprovalStatus:
    type: string
    enum:
      - approved
      - requested
      - denied
