#%RAML 1.0 Library

types:
  DatasetProvider:
    type: object
    properties:
      providerId:
        type: integer
        minimum: 1
      userId:
        type: integer
        minimum: 1
      projectId:
        type: integer
        minimum: 1
      datasetId:
        type: integer
        minimum: 1
      isManager: boolean
  DatasetProviderList:
    type: array
    items: DatasetProvider
